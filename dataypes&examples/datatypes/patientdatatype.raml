#%RAML 1.0 DataType

type: object
properties:
  patientId:
    type: string
  name:
    type: string
  age:
    type: integer
  email:
    type: string
    pattern: ^[_a-z0-9-]+(\.[_a-z0-9-]+)*@[a-z0-9-]+(\.[a-z0-9-]+)*(\.[a-z]{2,4})$
  phone:
    type: string
  gender:
    type: string
    enum: ["male", "female"]
  address:
    type: object
    properties:
      Country:
        type: string
      City:
        type: string
      Street:
        type: string
  room_service:
    type: boolean
